<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>CreateProductServlet</servlet-name>
  	<display-name>CreateProductServlet</display-name>
  	<description></description>
  	<servlet-class>com.ridham.products.servlets.CreateProductServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>DisplayProductsDetailServlet</servlet-name>
  	<display-name>DisplayProductsDetailServlet</display-name>
  	<description></description>
  	<servlet-class>com.ridham.products.servlets.DisplayProductsDetailServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>CreateProductServlet</servlet-name>
  	<url-pattern>/CreateProductServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>DisplayProductsDetailServlet</servlet-name>
  	<url-pattern>/DisplayProductsDetailServlet</url-pattern>
  </servlet-mapping>
</web-app>
